{
  "name": "bookipedia",
  "version": "0.0.1",
  "author": "Mihir Kumar",
  "description": "This is simple GraphQL implementation which is implemented using Express using Typescript in place of Javascript",
  "license": "ISC",
  "keywords": [
    "GraphQL",
    "Express",
    "TypeScript"
  ],
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/models",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.0",
    "http-status-codes": "^1.3.2",
    "module-alias": "^2.2.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.4.0",
    "@types/jsonfile": "^5.0.0",
    "@types/mongoose": "^5.5.13",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.4.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
